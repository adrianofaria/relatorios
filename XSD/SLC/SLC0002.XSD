            <cat:Emissor>Câmara SLC</cat:Emissor>
            <cat:Destinatario>IF-B</cat:Destinatario>
          </cat:InfMensagem>
        </xs:documentation>
      </xs:annotation>
    </xs:element>
  </xs:choice>
</xs:complexType>
 
