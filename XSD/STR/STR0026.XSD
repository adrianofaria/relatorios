          <cat:InfCampo>
            <cat:NomeCampo>Valor Outros Acréscimos</cat:NomeCampo>
            <cat:DescricaoCampo>Valor de outros acréscimos</cat:DescricaoCampo>
          </cat:InfCampo>
        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='CodIdentdTransf' type='CodIdentdTransf' minOccurs='0'>
