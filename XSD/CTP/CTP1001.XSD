<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>CTP1001 - IF consulta operação de Títulos</cat:Evento>
        <cat:Descricao>Destinado à IF consultar a operação de Títulos registrados na CETIP.</cat:Descricao>
        <cat:Observacao>O Número Controle Câmara Original deverá ser preenchido com o conteúdo do campo Número Controle LTR informado nas mensagens de liquidação bruta ou com o conteúdo do Número Controle CTP informado nas mensagens de respostado grupo CTP.</cat:Observacao>
        <cat:Servico>Consulta de Operações</cat:Servico>
        <cat:TipoFluxo>Fluxo4</cat:TipoFluxo>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
