            <cat:NomeCampo>Periodicidade Juros</cat:NomeCampo>
            <cat:DescricaoCampo>Frequência de pagamento de juros em prazo</cat:DescricaoCampo>
          </cat:InfCampo>
        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='TpAtvCTP' type='TpAtvCTP' minOccurs='0'>
      <xs:annotation>
        <xs:documentation>
