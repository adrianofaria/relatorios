  <xs:sequence>
    <xs:element name='CodMsg' type='CodMsg' fixed='PAG0111R2'>
      <xs:annotation>
        <xs:documentation>
          <cat:InfCampo>
            <cat:NomeCampo>Código Mensagem</cat:NomeCampo>
