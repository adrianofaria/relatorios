      <xs:annotation>
        <xs:documentation>
          <cat:InfCampo>
            <cat:NomeCampo>Preço_Unitário Colateral</cat:NomeCampo>
            <cat:DescricaoCampo>Preço Unitário da garantia.</cat:DescricaoCampo>
          </cat:InfCampo>
        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='IndrPrzLiquid' type='IndrPrazoLiquid'>
