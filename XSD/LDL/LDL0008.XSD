<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>LDL0008 - IF requisita Transferência para liquidação de eventos e emissões</cat:Evento>
        <cat:Descricao>Destinado à IF requisitar transferência de sua conta Reservas Bancárias para conta liquidação Câmara, decorrente de liquidação de eventos e emissões. Por exemplo: Pagamento de Juros, Dividendos, Subscrição, Resgate, Amortizações, Participações no Lucro, Pagamento Prêmio, Emissão, Não Repactuação.</cat:Descricao>
        <cat:Servico>Controle de Eventos e Emissões LDL</cat:Servico>
        <cat:TipoFluxo>Fluxo2</cat:TipoFluxo>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
