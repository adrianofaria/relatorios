<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>LDL1002 - Entidade Responsável requisita Aceite de depósito</cat:Evento>
        <cat:Descricao>Destinado à Entidade Responsável requisitar à Câmara o aceite de depósito, voluntário ou exigido, de garantia, de margem ou de títulos.</cat:Descricao>
        <cat:Servico>Gerência de Colateral ou de Movimentação de Títulos LDL</cat:Servico>
        <cat:TipoFluxo>Fluxo1</cat:TipoFluxo>
        <cat:InfRegra>
          <cat:CodigoRegra>RLDL0007</cat:CodigoRegra>
          <cat:DescricaoRegra>O evento permite a requisição de aceite de depósito com alocação de colateral para Entidade Responsável 2, o qual deverá ser indicado nos campos Código Titular e Código SubTitular.</cat:DescricaoRegra>
        </cat:InfRegra>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
