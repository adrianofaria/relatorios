    <xs:element name='Grupo_CSD1002_DivgcOpCSD' type='Grupo_CSD1002_DivgcOpCSDComplexType' maxOccurs='unbounded'>
      <xs:annotation>
        <xs:documentation>
          <cat:InfCampo>
            <cat:NomeCampo>Grupo Divergência Operação CSD</cat:NomeCampo>
