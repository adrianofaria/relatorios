<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>TES0015 - Tesouro requisita Transferência da Conta Única para conta corrente</cat:Evento>
        <cat:Descricao>Destinado ao Tesouro Nacional requisitar transferência de recursos da Conta Única para conta de um cliente de um Participante. Por exemplo: Pagamento de fornecedor.</cat:Descricao>
        <cat:Observacao>Esta mensagem poderá ser devolvida somente pela TES0016.</cat:Observacao>
        <cat:Servico>Controle de Débitos TES</cat:Servico>
        <cat:TipoFluxo>Fluxo2</cat:TipoFluxo>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
