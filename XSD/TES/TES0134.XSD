        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='TpRec' type='TpRecurso'>
      <xs:annotation>
        <xs:documentation>
          <cat:InfCampo>
            <cat:NomeCampo>Tipo Recurso</cat:NomeCampo>
            <cat:DescricaoCampo>Tipo do recurso</cat:DescricaoCampo>
          </cat:InfCampo>
