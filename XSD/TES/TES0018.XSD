<!-- Definicao da mensagem -->
 
<xs:element name='DOC' type='DOCComplexType'>
  <xs:annotation>
    <xs:documentation>
      <cat:InfEvento>
        <cat:Evento>TES0018 - Participante requisita Transferência para pagamento GPS</cat:Evento>
        <cat:Descricao>Destinado ao Participante requisitar transferência de reserva relativa a pagamento de GPS - Guia da Previdêcia Social próprio ou de cliente.</cat:Descricao>
        <cat:Observacao>Valor Encargo: atualização monetária, juros e multas.</cat:Observacao>
        <cat:Servico>Controle de Créditos TES</cat:Servico>
        <cat:TipoFluxo>Fluxo2</cat:TipoFluxo>
        <cat:InfRegra>
          <cat:CodigoRegra>RTES0005</cat:CodigoRegra>
          <cat:DescricaoRegra>Valor Lançamento é a soma dos campos Valor INSS, Valor Outras Entidades e Valor Encargos.</cat:DescricaoRegra>
        </cat:InfRegra>
      </cat:InfEvento>
    </xs:documentation>
  </xs:annotation>
</xs:element>
 
<xs:complexType name='DOCComplexType'>
  <xs:sequence>
    <xs:element name='BCMSG' type='BCMSGComplexType'/>
    <xs:element name='SISMSG' type='SISMSGComplexType'/>
    <xs:element name='USERMSG' type='USERMSGSimpleType' minOccurs='0'/>
  </xs:sequence>
</xs:complexType>
 
