        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='LTR0004R1' type='LTR0004R1ComplexType'>
      <xs:annotation>
        <xs:documentation>
          <cat:InfMensagem>
            <cat:Mensagem>Resposta ao Requisitante de Transferência Resultado Negociação</cat:Mensagem>
            <cat:Emissor>STR</cat:Emissor>
            <cat:Destinatario>IF</cat:Destinatario>
          </cat:InfMensagem>
        </xs:documentation>
      </xs:annotation>
    </xs:element>
    <xs:element name='LTR0004R2' type='LTR0004R2ComplexType'>
